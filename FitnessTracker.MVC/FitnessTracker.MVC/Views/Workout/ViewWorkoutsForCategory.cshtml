@{
    ViewData["Title"] = "Add Existing Workout";
}

@section css{
    <link rel="stylesheet" href="~/css/workout.css" asp-append-version="true" />
}

@model List<WorkoutModel>

<h2>@ViewBag.Category Workouts</h2>

<ul>
    @foreach (var workout in Model)
    {
        <li>
            <a href="@Url.Action("WorkoutDetails", "Workout", new { workoutName = workout.Name, category = ViewBag.Category })">@workout.Name</a>
        </li>
    }
</ul>