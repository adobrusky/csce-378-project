@{
    ViewData["Title"] = "Add Existing Workout";
}

@section css{
    <link rel="stylesheet" href="~/css/workout.css" asp-append-version="true" />
}

@model List<string>

<h2>Select a Category to Add an Existing Workout</h2>

<ul>
    @foreach (var category in Model)
    {
        <li>
            <a href="@Url.Action("ViewWorkoutsForCategory", "Workout", new { category = category })">@category</a>
        </li>
    }
</ul>